<?xml version="1.0" encoding="UTF-8"?>
<interface>
        <template class="PageOverlay" parent="GtkWidget">
                <property name="layout-manager">
                        <object class="GtkBinLayout"/>
                </property>
		<child>
			<object class="GtkOverlay" id="overlay">
				<property name="child">
					<object class="Page" id="page">
					</object>
				</property>
			</object>
		</child>
        </template>
</interface>

